    </nav>
</header>
<!--end top header-->

  <!--start page content-->

  <section class="section-padding bg-section-2">
    <div class="container">
      <div class="d-flex align-items-center">
        <div class="">
          <h3 class="mb-0 fw-bold">Search Products</h3>
        </div>
        <div class="ms-auto">
          <a href="/user/productList"> <button type="button" class="btn-close" aria-label="Close"></button></a>
        </div>
      </div>
      <div class="search-box position-relative mt-4">
        <form action="/user/searchProducts" method="post">
          <input class="form-control form-control-lg ps-5 rounded-0" type="text" name="text"
            placeholder="Type here to search">
          <input class="btn btn-primary mt-3" type="submit" value="Submit">
        </form>
      </div>

      <div class="product-grid mt-4">
         <% if (products.length===0){%>
          <h6 class="text-danger">Sorry we could'nt find anything</h6>
          <% } %>
            <div class="row row-cols-1 row-cols-md-2 row-cols-lg-3 g-4">
              <%for(i=0;i<products.length;i++) {%>
                <%if(products[i].isListed===true) {%>
                  <div class="col">
                    <div class="card border shadow-none">
                      <div class="position-relative overflow-hidden">
                        <div
                          class="product-options d-flex align-items-center justify-content-center gap-2 mx-auto position-absolute bottom-0 start-0 end-0">
                          <a href="javascript:;"><i class="bi bi-heart"></i></a>
                          <a href="javascript:;"><i class="bi bi-basket3"></i></a>
                          <a href="javascript:;"><i class="bi bi-zoom-in"></i></a>
                        </div>
                        <a href="/user/product-details?id=<%=products[i]._id%>">
                          <img src="/products/<%=products[i].images[0]%>" class="card-img-top" alt="...">
                        </a>
                      </div>
                      <div class="card-body border-top">
                        <h5 class="mb-0 fw-bold product-short-title">
                          <%=products[i].productName%>
                        </h5>
                        <p class="mb-0 product-short-name">
                          <%=products[i].catagory%>
                        </p>
                        <div class="product-price d-flex align-items-center gap-2 mt-2">
                          <div class="h6 fw-bold">
                            <%=products[i].price%>
                          </div>
                          <div class="h6 fw-light text-muted text-decoration-line-through">$2089</div>
                          <div class="h6 fw-bold text-danger">(70% off)</div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <%}%>
                    <%}%>
            </div><!--end row-->
      </div>
    </div>
  </section>

  <section class="section-padding">

  </section>

  <!--end page content-->



  <!-- JavaScript files -->
  <script src="/user/js/bootstrap.bundle.min.js"></script>
