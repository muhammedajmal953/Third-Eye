    <!--start product wishlist-->
    <section class="section-padding">
      <div class="container">
        <div class="d-flex align-items-center px-3 py-2 border mb-4">
          <div class="text-start">
            <h4 class="mb-0 h4 fw-bold">Wishlist </h4>
          </div>
          <div class="ms-auto">
            <button type="button" class="btn btn-light btn-ecomm">Continue Shopping</button>
          </div>
        </div>

        <div class="similar-products">
          <div class="row row-cols-1 row-cols-sm-2 row-cols-lg-3 row-cols-xl-4 row-cols-xxl-5 g-4">
            <% products.forEach(product=> {%>
              <div class="col">
                <div class="card rounded-0">
                  <button onclick="removeProduct('<%=product._id%>')"
                    class="btn-close wishlist-close position-absolute end-0 top-0"></button>
                  <a href="/user/product-details?id=<%=product._id%>">
                    <img src="/products/<%=product.images[0]%>" alt="" class="card-img-top rounded-0">
                  </a>
                  <div class="card-body border-top text-center">
                    <p class="mb-0 product-short-name">
                      <%=product.productName%>
                    </p>
                    <div class="product-price  align-items-center gap-2 mt-2 justify-content-center">
                      <div class="product-price d-flex align-items-center gap-2 mt-2">
                        <%let offerToApply=(product.pOffer && product.pOffer> product.cOffer) ?
                          product.pOffer : product.cOffer;
                          %>
                          <% if (offerToApply) { %>
                            <div class="h6 fw-bold">
                              $<%=Math.floor(product.price-(product.price*offerToApply /100))%>
                            </div>
                            <div class="h6 fw-light text-muted text-decoration-line-through">$
                              <%=product.price%>
                            </div>
                            <div class="h6 fw-bold text-danger">(<%=offerToApply%>% off)</div>
                            <% }else{ %>
                              <div class="h6 fw-bold">$ <%=product.price%>
                              </div>
                              <%}%>

                      </div>

                      <div class="h6 fw-bold text-danger">only <%=product.quantity%> left</div>
                    </div>
                  </div>
                  <div class="card-footer bg-transparent text-center">
                    <form action="/user/addToCart?productId=<%=product._id%>" method="post"> <button type="submit"
                        class="btn btn-ecomm w-100">Add to Bag</button></form>
                  </div>
                </div>
              </div>
              <% }) %>


          </div>
          <!--end row-->
        </div>
      </div>
    </section>
