    <!--start product grid-->
    <section class="section-padding">
      <h5 class="mb-0 fw-bold d-none">Product Grid</h5>
      <div class="container">
        <div class="btn btn-dark btn-ecomm d-xl-none position-fixed top-50 start-0 translate-middle-y"
          data-bs-toggle="offcanvas" data-bs-target="#offcanvasNavbarFilter"><span><i class="bi bi-funnel me-1"></i>
            Filters</span></div>
        <div class="row">
          <div class="col-12 col-xl-3 filter-column">
            <nav class="navbar navbar-expand-xl flex-wrap p-0">
              <div class="offcanvas offcanvas-start" tabindex="-1" id="offcanvasNavbarFilter"
                aria-labelledby="offcanvasNavbarFilterLabel">
                <div class="offcanvas-header">
                  <h5 class="offcanvas-title mb-0 fw-bold" id="offcanvasNavbarFilterLabel">Filters</h5>
                  <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas"
                    aria-label="Close"></button>
                </div>
                <div class="offcanvas-body">
                  <div class="filter-sidebar">
                    <div class="card rounded-0">
                      <div class="card-header d-none d-xl-block bg-transparent">
                        <h5 class="mb-0 fw-bold">Filters</h5>
                      </div>
                     <form action="" method="get">
                      <div class="card-body">
                        <h6 class="p-1 fw-bold bg-light">Categories</h6>
                        <div class="categories">
                          <div class="categories-wrapper height-1 p-1">
                            <%for(i=0;i<catagory.length;i++){%>
                              <%if(catagory[i].isActive===true){%>
                                <div class="form-check">
                                  <input class="form-check-input" name="category" type="checkbox" value="<%=catagory[i].catagoryName%>"
                                    id="chekCate1">
                                  <label class="form-check-label" for="chekCate1">
                                    <span>
                                      <%=catagory[i].catagoryName%>
                                    </span>
                                  </label>
                                </div>
                                <%}%>
                                  <%}%>
                          </div>
                        </div>
                        <hr>

                        <hr>
                        <div class="Price">
                          <!-- <h6 class="p-1 fw-bold bg-light">Price</h6> -->
                          <div class="Price-wrapper p-1">
                            <div class="input-group">
                              <!-- <input type="text" name="minPrice" class="form-control rounded-0" placeholder="$10"> -->
                              <span class="input-group-text bg-section-1 border-0">-</span>
                              <!-- <input type="text" name="maxPrice" class="form-control rounded-0" placeholder="$10000"> -->
                              <button type="submit" class="btn btn-outline-dark rounded-0 ms-2"
                               >Apply<i class="bi bi-chevron-right"></i></button>
                            </div>
                          </div>
                        </div>


                      </div>
                     </form>
                    </div>
                  </div>
                </div>
              </div>
            </nav>
          </div>
          <div class="col-12 col-xl-9">
            <div class="shop-right-sidebar">
              <div class="card rounded-0">
                <div class="card-body p-2">
                  <div class="d-flex align-items-center justify-content-between bg-light p-2">
                    <div class="product-count"></div>
                    <div class="view-type hstack gap-2 d-none d-xl-flex">
                      <p class="mb-0">Products</p>
                    </div>
                    <form>
                      <div class="input-group">
                        <span class="input-group-text bg-transparent rounded-0 border-0">Sort By</span>
                        <select class="form-select rounded-0" id="sortType" onchange="sortProducts('<%=page%>')">
                          <option value="popularity">
                            <%=sortBy%>
                          </option>
                          <option value="hiToLow">Price : Hight to Low</option>
                          <option value="lowToHi">Price : Low to Hight</option>
                          <option value="aToZ">Name:A-Z</option>
                          <option value="zToA">Name:Z-A</option>
                        </select>
                      </div>
                    </form>
                  </div>
                </div>
              </div>

              <div class="product-grid mt-4">
                <div class="row row-cols-1 row-cols-md-2 row-cols-lg-3 g-4">
                  <%for(i=0;i<products.length;i++) {%>
                    <%if(products[i].isListed===true&&products[i].quantity>0) {%>
                      <div class="col">
                        <div class="card border shadow-none">
                          <div class="position-relative overflow-hidden">
                            <a href="/user/product-details?id=<%=products[i]._id%>">
                              <img src="/products/<%=products[i].images[0]%>" class="card-img-top" alt="...">
                            </a>
                          </div>
                          <div class="card-body border-top">
                            <h5 class="mb-0 fw-bold product-short-title">
                              <%=products[i].productName%>
                            </h5>
                            <p class="mb-0 product-short-name">
                              <%=products[i].catagory%>
                            </p>
                            <div class="product-price d-flex align-items-center gap-2 mt-2">
                              <div class="h6 fw-bold">Rs-<%=products[i].price%>
                              </div>
                              <!-- <div class="h6 fw-light text-muted text-decoration-line-through">$2089</div>
                          <div class="h6 fw-bold text-danger">(70% off)</div> -->
                            </div>
                          </div>
                        </div>
                      </div>
                      <%}%>
                        <%}%>
                </div><!--end row-->
              </div>

              <hr class="my-4">

              <div class="product-pagination">
