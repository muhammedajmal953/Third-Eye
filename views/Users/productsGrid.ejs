                    <li class="page-item disabled">
                      <a class="page-link">Previous</a>
                    </li>
                    <% for(i=1;i<=totalPages;i++){ %>
                      <li class="page-item "><a class="page-link " href="/user/productList/<%=i%>">
                          <%=i%>
                        </a></li>
                      <% } %>
                        <a class="page-link" href="javascript:;">Next</a>
                        </li>
                  </ul>
                </nav>
              </div>

            </div>
          </div>
        </div><!--end row-->


      </div>
    </section>
